<%= form_for :user, url: users_path do |f| %>
    <div>
        <%= f.label :username %>
        <%= f.text_field :username %>
    </div>
    <div>
        <%= f.label :password %>
        <%= f.password_field :password %>
    </div>
    <div>
        <%= f.label :password_confirmation %>
        <%= f.password_field :password_confirmation %>
    </div>
    <div>
        <%= f.label :admin %>
        <%= f.check_box :admin %>
    </div>
    <%= f.submit %>
<% end %>

<%= button_to "Log in with Google", '/auth/google_oauth2'  %>